{
  "name": "generator-nest-js-boilerplate",
  "version": "2.2.1",
  "description": "This generator will help you to build your own Nest.js Mongodb API using TypeScript 4",
  "author": "onix-systems",
  "license": "MIT",
  "homepage": "https://onix-systems.com",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "node",
    "nodejs",
    "rest",
    "restfull",
    "api",
    "builder",
    "generator",
    "mongo",
    "ts",
    "starter",
    "mongodb",
    "auth",
    "js",
    "swagger"
  ],
  "repository": {
    "url": "https://github.com/Onix-Systems/nest-js-boilerplate"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git-cz"
  },
  "dependencies": {
    "@nestjs/common": "7.0.0",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.0.0",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.0.0",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/testing": "7.4.2",
    "@nestjs/typeorm": "7.1.0",
    "bcrypt": "5.0.0",
    "chalk": "4.1.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "connect-flash": "^0.1.1",
    "connect-mongodb-session": "^2.4.1",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "express": "4.17.1",
    "express-handlebars": "^5.1.0",
    "express-mysql-session": "^2.1.4",
    "express-session": "1.17.1",
    "hbs": "^4.1.1",
    "husky": "4.2.5",
    "ioredis": "4.17.3",
    "module-alias": "2.2.2",
    "mongodb": "3.5.9",
    "mongoose": "^5.10.4",
    "nestjs-redis": "1.2.8",
    "open": "7.2.1",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "print-message": "3.0.1",
    "redis": "^3.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "8.6.2",
    "typeorm": "0.2.25",
    "yeoman-generator": "4.12.0",
    "yosay": "2.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "7.0.0",
    "@nestjs/schematics": "7.3.1",
    "@types/connect-flash": "0.0.35",
    "@types/connect-redis": "0.0.14",
    "@types/express": "4.17.3",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-session": "^1.17.0",
    "@types/jest": "25.2.3",
    "@types/mongoose": "^5.7.36",
    "@types/node": "13.9.1",
    "@types/passport-google-oauth20": "2.0.3",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/redis": "^2.8.27",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "26.0.1",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "6.2.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
